{
  "$schema": "https://json.schemastore.org/package",
  "name": "retrieve-maven-artifact-versions",
  "version": "1.0.0",
  "engines": {
    "node": ">=16"
  },
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint --fix",
    "test": "jest --passWithNoTests",
    "tsconfig-generate-app": "node tsconfig-generate-app",
    "compile": "npm run lint && rimraf build && node tsconfig-generate-app && tsc -project tsconfig.app.json --rootDir src --outDir build",
    "dist": "npm run compile && rimraf dist && ncc build build/main.js --out dist",
    "build": "npm run dist && npm run test"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/http-client": "2.1.0",
    "ts-retry-promise": "0.7.0",
    "xml-js": "1.6.11"
  },
  "devDependencies": {
    "@tsconfig/node12": "1.0.11",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-github": "4.7.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.5.0",
    "jest-circus": "29.5.0",
    "nock": "13.3.0",
    "rimraf": "4.4.1",
    "ts-jest": "29.1.0",
    "typescript": "5.0.3"
  }
}
