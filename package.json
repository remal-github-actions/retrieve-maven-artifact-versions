{
  "$schema": "https://json.schemastore.org/package",
  "name": "retrieve-maven-artifact-versions",
  "version": "1.0.0",
  "engines": {
    "node": ">=12"
  },
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint --fix",
    "test": "jest --passWithNoTests",
    "tsconfig-generate-app": "node tsconfig-generate-app",
    "compile": "npm run lint && rimraf build && node tsconfig-generate-app && tsc -project tsconfig.app.json --rootDir src --outDir build",
    "dist": "npm run compile && rimraf dist && ncc build build/main.js --out dist",
    "build": "npm run dist && npm run test"
  },
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/http-client": "1.0.11",
    "ts-retry-promise": "0.6.0",
    "xml-js": "1.6.11"
  },
  "devDependencies": {
    "@tsconfig/node12": "1.0.9",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "@vercel/ncc": "0.31.1",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-github": "4.3.2",
    "eslint-plugin-jest": "25.2.2",
    "jest": "27.3.1",
    "jest-circus": "27.3.1",
    "nock": "13.1.4",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.7",
    "typescript": "4.3.5"
  }
}
